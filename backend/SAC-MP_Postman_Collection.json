      "name": "1. Authentication",
      "item": [
        {
          "name": "1.1 Register Patient",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test(\"Status code is 201 Created\", function () {",
                  "    pm.response.to.have.status(201);",
                  "});",
                  "",
                  "pm.test(\"Response has access token\", function () {",
                  "    var jsonData = pm.response.json();",
                  "    pm.expect(jsonData.accessToken).to.be.a('string');",
                  "    pm.expect(jsonData.refreshToken).to.be.a('string');",
                  "    ",
                  "    // Sauvegarder les tokens",
                  "    pm.collectionVariables.set(\"access_token\", jsonData.accessToken);",
                  "    pm.collectionVariables.set(\"refresh_token\", jsonData.refreshToken);",
                  "    ",
                  "    // Sauvegarder l'ID utilisateur",
                  "    if (jsonData.user && jsonData.user.id) {",
                  "        pm.collectionVariables.set(\"patient_user_id\", jsonData.user.id);",
                  "    }",
                  "});",
                  "",
                  "pm.test(\"User role is PATIENT\", function () {",
                  "    var jsonData = pm.response.json();",
                  "    pm.expect(jsonData.user.role).to.eql(\"PATIENT\");",
                  "});"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"fullName\": \"Jean Dupont\",\n  \"email\": \"jean.dupont@test.com\",\n  \"password\": \"SecurePass123!\",\n  \"confirmPassword\": \"SecurePass123!\",\n  \"role\": \"PATIENT\",\n  \"phoneNumber\": \"+33612345678\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/v1/auth/register",
              "host": ["{{base_url}}"],
              "path": ["v1", "auth", "register"]
            }
          },
          "response": []
        },
