    {
      "name": "5. Alert API",
      "item": [
        {
          "name": "5.1 Get All Active Alerts",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test(\"Status code is 200 OK\", function () {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "",
                  "pm.test(\"Alerts are paginated\", function () {",
                  "    var jsonData = pm.response.json();",
                  "    pm.expect(jsonData).to.have.property('content');",
                  "    ",
                  "    // Sauvegarder le premier alert_id si disponible",
                  "    if (jsonData.content && jsonData.content.length > 0) {",
                  "        pm.collectionVariables.set(\"alert_id\", jsonData.content[0].id);",
                  "    }",
                  "});"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{hw_access_token}}"
